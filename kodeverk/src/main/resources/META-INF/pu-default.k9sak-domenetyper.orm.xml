<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
	xmlns="https://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://java.sun.com/xml/ns/persistence/orm
    https://java.sun.com/xml/ns/persistence/orm/orm_3_0.xsd"
	version="3.0">

	<!-- mapping av domene typer til JPA (optional) -->

	<embeddable class="no.nav.k9.sak.typer.Saksnummer" 
		access="FIELD">
		<attributes>
			<basic name="saksnummer" access="FIELD">
				<column name="saksnummer" />
			</basic>
		</attributes>
	</embeddable>

	<embeddable class="no.nav.k9.sak.typer.AktørId"
		access="FIELD">
		<attributes>
			<basic name="aktørId" access="FIELD">
				<column name="aktoer_id" />
			</basic>
		</attributes>
	</embeddable>

	<embeddable class="no.nav.k9.sak.typer.JournalpostId"
		access="FIELD">
		<attributes>
			<basic name="journalpostId" access="FIELD">
				<column name="journalpost_id" />
			</basic>
		</attributes>
	</embeddable>

	<embeddable class="no.nav.k9.sak.typer.PersonIdent"
		access="FIELD">
		<attributes>
			<basic name="ident" access="FIELD">
				<column name="ident" />
			</basic>
		</attributes>
	</embeddable>

	<embeddable class="no.nav.k9.sak.typer.Stillingsprosent"
		access="FIELD">
		<attributes>
			<basic name="verdi" access="FIELD">
				<column name="verdi" />
			</basic>
		</attributes>
	</embeddable>

	<embeddable
		class="no.nav.k9.sak.typer.EksternArbeidsforholdRef" access="FIELD">
		<attributes>
			<basic name="referanse" access="FIELD">
				<column name="arbeidsforhold_id" />
			</basic>
		</attributes>
	</embeddable>


	<embeddable
		class="no.nav.k9.sak.typer.InternArbeidsforholdRef" access="FIELD">
		<attributes>
			<basic name="referanse" access="FIELD">
				<column name="arbeidsforhold_intern_id" />
			</basic>
		</attributes>
	</embeddable>

	<embeddable class="no.nav.k9.sak.typer.OrgNummer"
		access="FIELD">
		<attributes>
			<basic name="orgNummer" access="FIELD">
				<column name="org_nummer" updatable="false" length="50" />
			</basic>
		</attributes>
	</embeddable>


	<embeddable class="no.nav.k9.sak.typer.Arbeidsgiver" access="FIELD">
		<attributes>
			<basic name="arbeidsgiverOrgnr" access="FIELD">
				<column name="arbeidsgiver_orgnr" updatable="false" length="50" />
			</basic>
			<embedded name="arbeidsgiverAktørId" access="FIELD">
				<attribute-override name="aktørId">
					<column name="arbeidsgiver_aktor_id" updatable="false" length="50" />
				</attribute-override>
			</embedded>
		</attributes>
	</embeddable>
	
</entity-mappings>
