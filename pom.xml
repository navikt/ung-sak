        <url>https://github.com/navikt/ung-sak</url>
        <tag>HEAD</tag>
    </scm>
    <distributionManagement>
        <repository>
            <id>github</id>
            <name>Github navikt Maven Packages</name>
            <url>https://maven.pkg.github.com/navikt/ung-sak</url>
        </repository>
    </distributionManagement>
</project>
